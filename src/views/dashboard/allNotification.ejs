<% if(allNotification && allNotification.length !==0) {%>
  <ul class="list-group">
    <% for(var i=0; i < allNotification.length; i++) { %>
      <li class="list-group-item list-group-item-light">
        <%= allNotification[i].message %>&nbsp; <span class="float-end">
            <% if(allNotification[i].read_status===1) { %><i class="fas fa-envelope-open"
                onclick="updateNotificationReadStatus0('<%= allNotification[i].notification_id %>')"></i>
              <% }else { %><i class="fas fa-envelope"
                  onclick="updateNotificationReadStatus1('<%= allNotification[i].notification_id %>')"></i>
                <% } %>
          </span>
      </li>
      <%} %>
  </ul>
  <% }else { %>

    <p>No notification data found!</p>

    <% } %>